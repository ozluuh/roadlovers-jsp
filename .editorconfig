# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
end_of_line = lf
trim_trailing_whitespace = true
insert_final_newline = true

[*.{java,xml,tld}]
indent_style = tab
indent_size = 4
charset = utf-8

[*.{tag,js,jsp}]
indent_style = space
indent_size = 4
charset = utf-8

[*.css]
indent_style = space
indent_size = 2
charset = utf-8

[*.{min.css,svg}]
insert_final_newline = unset
charset = utf-8
